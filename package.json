{
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register",
      "./test/setup.js",
      "ignore-styles"
    ],
    "babel": "inherit",
    "tap": true
  },
  "main": "./dist/undo-redo-vuex.min.js",
  "name": "undo-redo-vuex",
  "version": "1.1.4",
  "description": "A Vuex plugin for module namespaced undo and redo functionality.",
  "author": "Andrew Beng <andrewbeng89@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "@ava/babel-preset-transform-test-files": "^4.0.0-beta.3",
    "ava": "^0.25.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-env-2": "^1.5.2",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "browser-env": "^3.2.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "fast-deep-equal": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "jsdoc-to-markdown": "^4.0.1",
    "prettier": "^1.12.1",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-module": "^0.1.4",
    "require-extension-hooks-vue": "^1.0.0",
    "rimraf": "^2.6.2",
    "vue": "^2.5.16",
    "vue-plain": "^0.1.1",
    "vuex": "^3.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2"
  },
  "keywords": [
    "vuex",
    "vue",
    "undo/redo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewbeng89/undo-redo-vuex.git"
  },
  "scripts": {
    "build": "rimraf ./dist && webpack --optimize-minimize --config ./webpack.config.js",
    "test": "ENV=test ava ./test/test.js ./test/test-non-namespaced.js ./test/test-multiple-undo.js ./test/test-action-group-undo.js ./test/test-advance-action-groups.js",
    "test:actionGroup": "ENV=test ava ./test/test-action-group-undo.js",
    "test:advanceActionGroups": "ENV=test ava ./test/test-advance-action-groups.js",
    "test:basic": "ENV=test ava ./test/test.js",
    "test:nonNamespaced": "ENV=test ava ./test/test-non-namespaced.js",
    "test:multipleUndo": "ENV=test ava ./test/test-multiple-undo.js",
    "doc-to-markdown": "jsdoc2md ./src/undoRedo.js"
  }
}
